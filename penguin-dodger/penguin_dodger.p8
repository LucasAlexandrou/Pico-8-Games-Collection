--colission

function water_colission()
	local left=flr((player.x+3)/8)
	local right=flr((player.x+3)/8)
	local top=flr((player.y+6)/8)
	local bottom=flr((player.y+3)/8)
	
	local tiles = {
		mget(left,top),
		mget(right,top),
		mget(left,bottom),
		mget(right,bottom)
	}
	
	local water_hits=0
	for t in all(tiles) do
	 if t == 33 or t == 34 
	  or t == 35 or t == 36
	  or t == 37 or t == 38
	  or t == 39 then
	  	water_hits+=1
	 end
 end
 
 if water_hits>=2 then
 	create_splash(player.x,player.y)
	 sfx(4)
	 music(-1)
	 state="waiting"
	end
end

function check_bullet_colission()
 for b in all(bullets) do
 	local dx=b.x-(player.x+4)
 	local dy=b.y-(player.y+4)
 	local dist=sqrt(dx*dx + dy*dy)
 	
 	if dist<6.5 then 
 	 sfx(0)
 	 state="dead"
 	 if toggle_music==true then
 	  music(6)
 	 end
 	end
 end
end

