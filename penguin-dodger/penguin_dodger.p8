--reset

function reset_game()
	player.sp=60
	player.x=61
	player.y=60
	score_timer=0
 score_counter=0
 game_over_timer=20
 difficulty=1
 difficulty_timer=0
 for s in all(seals) do
  s.x=-99
  s.y=-99
  s.active=false
  s.timer=0
  s.delay=flr(rnd(120))+60
 end
 bullets={}
 
 music(-1)
 sfx(-1)
 current_audio=nil
end
