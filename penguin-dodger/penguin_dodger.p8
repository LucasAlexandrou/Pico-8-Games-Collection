--enemies

function create_seals()
	seals = {
	 make_seal(92,rand_seal1),
	 make_seal(124,rand_seal2),
	 make_seal(76,rand_seal3),
	 make_seal(108,rand_seal4)
	}
	
	bullets = {}
	difficulty=1
	difficulty_timer=0
end

function make_seal(sp,rand_func)
 return {
	sp=sp,
	shoot_sp=sp+1,
	x=-99,
	y=-99,
	timer=0,
	delay=flr(rnd(120))+60,
	active=false,
	rand_func=rand_func
	}
end

function update_seals()
	difficulty_timer+=1
	if difficulty_timer%300 == 0 then
		difficulty+=0.35
	end
	
	for s in all(seals) do
	 s.timer+=1
	 
	 if not s.active and s.timer>s.delay/difficulty then
	 	s.rand_func(s)
	 	create_splash(s.x,s.y)
	 	s.active=true
	 	s.timer=0
	 	sfx(4)
	 end
	 
	 if s.active and s.timer==flr(30/difficulty) then
	 	s.sp=s.shoot_sp
	 	shoot_bullet(s.x,s.y,player.x,player.y)
			sfx(6)
		end
			
		if s.active and s.timer>flr(40/difficulty) then
		 s.sp=s.shoot_sp-1
		end
		
		if s.active and s.timer>flr(60/difficulty) then
			s.x=-99
			s.y=-99
			s.active=false
			s.timer=0
			s.delay=flr(rnd(120))+60
		end
	end
	move_bullets()
	check_bullet_colission()
end

function draw_seals()
 for s in all(seals) do
 	if s.active then
 	 spr(s.sp,s.x,s.y)
 	end
 end

	for b in all(bullets) do
		circfill(b.x,b.y,2,10)
	end
end

function shoot_bullet(x,y,tx,ty)
	local dx = tx-x
	local dy = ty-y
	local dist = sqrt(dx*dx + dy*dy)
	local spd=1.2+(difficulty-1)*0.2
	
	add(bullets, {
	x=x,
	y=y,
	xs=(dx/dist)*spd,
	ys=(dy/dist)*spd
	})
end

function move_bullets()
 for b in all(bullets) do 
 	b.x+=b.xs
 	b.y+=b.ys
 	if b.x<0 or b.x>128
 	or b.y<0 or b.y>128 then
 	 del(bullets,b)
 	end
 end
end

function rand_seal1(s)
	s.x = flr(rnd(18))
	s.y = flr(rnd(120))
end

function rand_seal2(s)
	s.x = 32 + flr(rnd(64))
	s.y = 96 + flr(rnd(24))
end

function rand_seal3(s)
	s.x = 104+flr(rnd(16))
	s.y = flr(rnd(120))
end

function rand_seal4(s)
	s.x = 32 + flr(rnd(64))
	s.y = 4+flr(rnd(20))
end

