function _init()
	cartdata("save_1")
 state = "menu"
 palt(0,false)
 palt(15,true)
 music(0)
 
 wave_timer = 0
 score_timer = 0
 score_counter = 0
 highscore=dget(0)

 player = {
	 sp = 64,
	 x = 61,
	 y = 60,
	 stimer=0,
	 direction="down",
	 last_dir="down"
 }

 laughseal1 = { 
 	sp = 77, 
 	x = 96,
	 y = 72,
	 stimer=0
 }

 laughseal2 = {
	 sp = 93,
	 x = 28 ,
	 y = 80,
	 stimer=0
 }

	snow={}
	splash={}
	create_seals()
	laughs={}
	game_over_timer=20
	laugh_timer=0
	toggle_music=true
end

function _update()
 wave_timer = (wave_timer+1)%64
	
	if state == "menu" then
		if btnp(‚ùé) then
			reset_game()
		 state = "game"
		 if toggle_music==true then
		  music(4)
		 end
	 end
	 if btnp(üÖæÔ∏è) then
	  state = "settings"
	 end
 elseif state=="settings" then
 	if btnp(üÖæÔ∏è) then
 	 state="menu"
 	end
 	if btnp(‚ùé) then
 		if toggle_music==true then
 		 toggle_music=false
 		elseif toggle_music==false then
 		 toggle_music=true
 		end
 	end
 end
 
 if state=="game" or state=="waiting" then
	 if state=="game" then
	  player_controls()
	  score_counter+=1
		 move_snowp()
		 
		 water_colission()
		
			update_seals()
			
	  if score_counter==30 then
	 	 score_timer+=1
	 	 score_counter=0
	  end
	 end
	 move_splash()
	end
	
	if state == "waiting" then
		game_over_timer-=1
		if game_over_timer<0 then
		 state="dead"
		 if toggle_music==true then
		  music(6)
		 end
		end
	end
	
 if state == "dead" then
  if score_timer>highscore then
	  	highscore = score_timer
	  	dset(0,highscore)
	 end
 	animate_laughseals()
 	move_laughs()
 	laugh_timer+=1
 	if (laugh_timer%20==0) then 
 	 create_laughs()
 	end
 	
	 if btnp(‚ùé) then
	 	reset_game()
		 state = "game"
		 if toggle_music==true then
		  music(4)
		 end
	 end
 	if btnp(üÖæÔ∏è) then
	 	state = "menu"
	 	if toggle_music==true then
	 	 music(0)
	 	end
 	end
 end
end

function _draw()
	cls()
	local offset = sin(wave_timer/64)*2
	
	if state == "menu" then
	 map(16,0)
	 
	 wave_motion(offset)

	 print("penguin dodger",64-(#"penguin dodger"*2),28,5)
	 print("by lethaldawg",64-(#"by lethaldawg"*2),36,5)
  print("press ‚ùé to start",64-(#"press ‚ùé to start"*2),60,5)
	 print("press üÖæÔ∏è for settings",64-(#"press üÖæÔ∏è for settings"*2),70,5)
	end
	
	if state=="settings" then
	 map(16,0)
	 
  print("settings",64-(#"settings"*2),30,5)
  print("press üÖæÔ∏è to return to menu",64-(#"press üÖæÔ∏è to return to menu"*2),40,5)
  print("press ‚ùé to toggle music",64-(#"press ‚ùé to toggle music"*2),50,5)
  if toggle_music==true then
   print("music:on",64-(#"music:on"*2),60,5)
  elseif toggle_music==false then
   print("music:off",64-(#"music:off"*2),60,5)
  end
	end
	
	if state == "game" or state == "waiting" then
		map(0,0)
		
		wave_motion(offset)
		
		for s in all(snow) do
			circfill(s.x,s.y,s.r,s.c)
		end
		
		for s in all(splash) do
		 circfill(s.x,s.y,s.r,s.c)
		end
		
		print("score:"..score_timer,64-(#("score:"..score_timer)*2),0,7)
		spr(player.sp,player.x,player.y)
		draw_seals()
	end
	
	if state == "dead" then
	 map(32,0)
	 
	 wave_motion(offset)
	 
	 for l in all(laughs) do
	 	print("ha",l.x,l.y,5)
	 end
	 
	 print("game over",64-(#"game over"*2),32,5)
	 print("score:"..score_timer,64-(#("score:"..score_timer)*2),40,5)
	 print("highscore:"..highscore,64-(#("highscore:"..highscore)*2),48,5)
	 print("press üÖæÔ∏è to return to menu",64-(#"press üÖæÔ∏è to return to menu"*2),92,5)
  print("press ‚ùé to play again",64-(#"press ‚ùé to play again"*2),100,5)
	 spr(laughseal1.sp,laughseal1.x,laughseal1.y)
	 spr(laughseal2.sp,laughseal2.x,laughseal2.y)
	end
end
