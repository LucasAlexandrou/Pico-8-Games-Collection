--particles

function create_snowp()
	for i=1,2 do
	 add(snow,{
	  x=player.x+rnd(6),
	  y=player.y+6+rnd(3),
	  speed=rnd(1),
	  c=5,
	  r=rnd(1),
	  l=3
	 })
	end
end

function move_snowp()
	 for s in all(snow) do
	 	s.x+=s.speed
	 	s.l-=1
	 	if s.l<0 then
	 	 del(snow,s)
	 	end
	 end
end

function create_splash(sx,sy,colour)
	for i=1,10 do
	 add(splash,{
	  x=sx+rnd(8)-4,
	  y=sy+rnd(8)-4,
	  xspeed=rnd(2)-1,
	  yspeed=-rnd(2)-0.5,
	  gravity=0.08,
	  c=(rnd(1)<0.5) and 1 or 12,
	  r=rnd(3),
	  l=20
	 })
	end	
end

function move_splash()
 for s in all(splash) do
	 	s.x+=s.xspeed
	 	s.y+=s.yspeed
	 	s.yspeed+=s.gravity
	 	s.l-=1
	 	
	 	if s.l<15 then
	 	 s.r-=0.1
	 		if s.r<0 then
	 		 s.r=0
	 		end
	 	end
	 	
	 	if s.l<0 then
	 	 del(splash,s)
	 	end
	 end
end

function create_laughs()
	 add(laughs,{
	  x=laughseal1.x-4,
	  y=laughseal1.y-4,
	  xspeed=rnd(0.5)-0.25,
	  yspeed=-0.5-rnd(0.3),
	  l=40
	 })
	 
	 add(laughs,{
	  x=laughseal2.x+4,
	  y=laughseal2.y-4,
	  xspeed=rnd(0.5)-0.25,
	  yspeed=-0.5-rnd(0.3),
	  l=40
	 })
end

function move_laughs()
	 for l in all(laughs) do
	 	l.x+=l.xspeed
	 	l.y+=l.yspeed
	 	l.l-=1
	 	
	 	if l.l<20 then
	 		l.yspeed-=0.01
	 	end
	 	
	 	if l.l<0 then
	 	 del(laughs,l)
	 	end
	 end
end
