-- animation
function set_direction(new_dir)
	if player.direction!=new_dir then
		player.direction = new_dir
	end
	
	if player.last_dir!=player.direction then
		player.last_dir = player.direction
		player.stimer=0
		
		if player.direction=="down" then
			player.sp=64
		elseif player.direction=="up" then
		 player.sp=80
		elseif player.direction=="right" then
			player.sp=96
		elseif player.direction=="left" then
		 player.sp=112
		end
	end
end
	
		
function animate_player()
	if player.direction=="down" then animate_dir(64)
	elseif player.direction=="up" then animate_dir(80)
	elseif player.direction=="right" then animate_dir(96)
	elseif player.direction=="left" then animate_dir(112)
	end
end


function animate_dir(spstart)
	player.stimer+=1
	
	if player.stimer >=8 then
		player.stimer=0
	
		if player.sp < spstart+3 then
			player.sp+=1
		else
			player.sp=spstart
		end
	end
end

function idle_animation()
	player.stimer+=1
	local base_sp=64
	
	if player.direction=="up" then
	 base_sp=80
	elseif player.direction=="right" then
	 base_sp=96
	elseif player.direction=="left" then
	 base_sp=112
	end
	
	local sway = sin(wave_timer/90)
	
	if sway>0 then
		player.sp=base_sp+1
	else
		player.sp=base_sp
	end
end

function wave_motion(offset)
 for ty=0,15 do
		 for tx=0,15 do
			 local tile=mget(tx,ty)
			  if tile == 33 or tile == 34 
			   or tile == 35 or tile == 36
			 	 or tile == 37 or tile == 38 then
			 		spr(tile,tx*8+offset,ty*8)
			 	end
			end
		end
end

function animate_laughseals()
	
	if laughseal1.stimer<10 then
		laughseal1.stimer+=1
	else
		if laughseal1.sp<78
			then laughseal1.sp+=1
		else
			laughseal1.sp=77
		end
	laughseal1.stimer=0
	end
		
	if laughseal2.stimer<10 then
		laughseal2.stimer+=1
	else
		if laughseal2.sp<94
			then laughseal2.sp+=1
		else
			laughseal2.sp=93
		end
			laughseal2.stimer=0
	end

end
